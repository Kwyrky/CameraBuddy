		<file src="CameraBuddy.WindowsUniversal\bin\Release\CameraBuddy.WindowsUniversal.pdb" target="lib\uap10.0\CameraBuddy.WindowsUniversal.pdb" />
		<file src="CameraBuddy.WindowsUniversal\bin\Release\CameraBuddy.WindowsUniversal.dll" target="lib\uap10.0\CameraBuddy.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="CameraBuddy.DesktopGL\bin\Release\CameraBuddy.DesktopGL.pdb" target="lib\net451\CameraBuddy.DesktopGL.pdb" />
		<file src="CameraBuddy.DesktopGL\bin\Release\CameraBuddy.DesktopGL.dll" target="lib\net451\CameraBuddy.DesktopGL.dll" />

